snmpwalk -c 'password' -v 2c 172.31.130.18 .1.3.6.1.2.1.17.4.3.1.2 >/home/switches/130.18-p

snmpwalk -c 'password' -v 2c 172.31.130.19 .1.3.6.1.2.1.17.4.3.1.1 >/home/switches/130.19-m
snmpwalk -c 'password' -v 2c 172.31.130.19 .1.3.6.1.2.1.17.4.3.1.2 >/home/switches/130.19-p

